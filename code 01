#define beat 323
#define pin 8
#define eight 0.5
#define sixteenth 0.25
#define Do2 294
#define Re2 330
#define Mi2 370
#define Fa2 392
#define So2 440
#define La2 494
#define Si2 554

#define Do1 147
#define Re1 165
#define Mi1 185
#define Fa1 196
#define So1 220
#define La1 247
#define Si1 277
void setup() {
  // put your setup code here, to run once:
  pinMode(pin,OUTPUT);
}
void ttone(int a,int b,int c)
{
  tone(a,b,c);
  delay(c);
  noTone(a);
}
void loop() {
  delay(beat*3);
ttone(pin,Mi2,625*eight);
ttone(pin,So2,625*eight);

ttone(pin,La2,625);
ttone(pin,Mi2,625*eight);
ttone(pin,So2,625*eight);
ttone(pin,La2,625);
ttone(pin,La2,625*eight);
ttone(pin,Mi2*2,625*eight);

