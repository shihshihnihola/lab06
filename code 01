ttone(pin,Mi2*2,625*eight);
ttone(pin,Re2*2,625*eight);
ttone(pin,Do2*2,625*eight);
ttone(pin,La2,625);
ttone(pin,So2,625*eight);
ttone(pin,Mi2,625*eight);

ttone(pin,Re2,625*eight);
ttone(pin,La1,625*eight);
ttone(pin,Re2,625*eight);
ttone(pin,Mi2,625*eight);
ttone(pin,Re2,625*eight+625*sixteenth);
ttone(pin,Mi2,625*sixteenth);
ttone(pin,Re2,625*sixteenth);
ttone(pin,Do2,625*eight);
ttone(pin,Re2,625*eight+beat*2);
//二
delay(beat);
ttone(pin,Mi1,beat*sixteenth);
ttone(pin,So1,beat*sixteenth);
ttone(pin,La1,beat*sixteenth);
ttone(pin,Do2,beat*sixteenth);

